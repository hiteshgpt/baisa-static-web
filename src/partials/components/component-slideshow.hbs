<!-- slideshow content begin -->
<div class="uk-section uk-padding-remove-vertical">
    <div class="uk-light in-slideshow uk-background-cover uk-background-top-center" style="background-image: url({{root}}img/in-liquid-slide-bg.png);" data-uk-slideshow>        
        <ul class="uk-slideshow-items">
            {{> module-slideshow-1}}
            {{> module-slideshow-2}}
            {{!> module-slideshow-3}}
            {{!> module-slideshow-4}}
        </ul>
        <a class="uk-position-center-left uk-position-small uk-hidden-hover" href="#" data-uk-slidenav-previous data-uk-slideshow-item="previous"></a>
        <a class="uk-position-center-right uk-position-small uk-hidden-hover" href="#" data-uk-slidenav-next data-uk-slideshow-item="next"></a>
        {{> module-slideshow-feature-2}}
        {{!> module-slideshow-feature-1}}
        {{!> module-slideshow-feature-3}}
        {{!> module-slideshow-feature-4}}

    </div>
</div>
<!-- slideshow content end -->
